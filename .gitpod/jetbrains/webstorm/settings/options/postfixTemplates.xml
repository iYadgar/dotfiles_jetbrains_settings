    </template>
    <template id="ct@userDefined" key=".time" provider="builtin.js" language-level="ES6" topmost="false">
      <conditions />
      <template name="fakeKey" value="console.time('$EXPR$')" description="" toReformat="true" toShortenFQNames="true" />
