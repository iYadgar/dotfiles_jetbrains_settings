<application>
  <component name="PostfixTemplates">
    <template id="ctt@userDefined" key=".ctt" provider="builtin.js" language-level="ES6" topmost="false">
      <conditions />
      <template name="fakeKey" value="console.time('$EXPR$')&#10;console.timeEnd('$EXPR$')" description="" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="llg@userDefined" key=".llg" provider="builtin.js" language-level="ES6" topmost="false">
      <conditions />
      <template name="fakeKey" value="$EXPR$((x)=&gt;console.log(&quot;$EXPR$: &quot;+ x))" description="" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="com.intellij.lang.javascript.template.postfix.JSLogPostfixTemplate#.log" key=".log" provider="builtin.js" builtin="com.intellij.lang.javascript.template.postfix.JSLogPostfixTemplate#.log" />
    <template id="cll@userDefined" key=".cll" provider="builtin.js" language-level="ES6" topmost="false">
